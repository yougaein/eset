    "editor.tokenColorCustomizations": {
        "comments": {
          "foreground": "#00cf00",
        },
        "functions": {
          "foreground": "#ffffff",
        },
        "keywords": {
            "foreground": "#ffff00",
          },
        "numbers": "#00e1ff",
        "strings": "#00ffff",
        "types": "#ffffff",
        "variables": "#ffffff",
        "textMateRules": [
            {
                "scope": "storage.modifier",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "keyword",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "storage.type",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "variable.language.this",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "keyword.other.operator",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "entity.name.scope-resolution",
                "settings" :{
                    "foreground": "#eeff00",
                }
            },
            {
                "scope": "meta.preprocessor",
                "settings" :{
                    "foreground": "#a5a5a1",
                    "fontStyle": "bold"
                }
            }
        ]
    },
